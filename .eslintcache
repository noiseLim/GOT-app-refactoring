ctoring\\src\\components\\randomChar\\randomChar.js",["103"],"import React, {useState, useEffect} from 'react';\r\nimport './randomChar.css';\r\nimport GotService from '../../services/gotService';\r\nimport Spinner from '../spinner';\r\nimport ErrorMessage from '../errorMessage';\r\n\r\nfunction RandomChar() {\r\n    const [char, updateChar] = useState({});\r\n    const [loading, onLoading] = useState(true);\r\n    const [error, onError] = useState(false);\r\n\r\n    const gotService = new GotService();\r\n\r\n    useEffect(() => {\r\n        updateCharacter();\r\n        const timerID = setInterval(updateCharacter, 15000);\r\n            return () => clearInterval(timerID);\r\n    }, [])\r\n\r\n    const updateCharacter = () => {\r\n        const id = Math.floor(Math.random()*140 + 25);\r\n        gotService.getCharacter(id)\r\n            .then((data) => {\r\n                updateChar(data);\r\n                onLoading(false);\r\n            })\r\n            .catch(() => {\r\n                onError(true);\r\n                onLoading(false);\r\n            });\r\n    }\r\n\r\n    const errorMessage = error ? <ErrorMessage/> : null;\r\n    const spinner = loading ? <Spinner/> : null;\r\n    const content = !(loading || error) ? <View char={char}/> : null;\r\n    \r\n    return (\r\n        <div className=\"random-block rounded\">\r\n            {errorMessage}\r\n            {spinner}\r\n            {content}\r\n        </div>\r\n    );\r\n}\r\n\r\nconst View = ({char}) => {\r\n    const {name, gender, born, died, culture} = char;\r\n    return (\r\n        <>\r\n            <h4>Random Character:</h4>\r\n            <h4>{name}</h4>\r\n            <ul className=\"list-group list-group-flush\">\r\n                <li className=\"list-group-item d-flex justify-content-between\">\r\n                    <span className=\"term\">Gender </span>\r\n                    <span>{gender}</span>\r\n                </li>\r\n                <li className=\"list-group-item d-fle